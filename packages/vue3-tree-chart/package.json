{
  "name": "@ssthouse/vue3-tree-chart",
  "author": "ssthouse",
  "version": "0.2.6",
  "description": "Tree Component for Vue3 powered by D3.js & SVG",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "release": "npm run build:component && npm publish",
    "build:component": "vue-cli-service build --target lib --name vue3-tree-chart src/vue-tree/index.js"
  },
  "main": "dist/vue3-tree-chart.umd.js",
  "dependencies": {
    "@ssthouse/tree-chart-core": "^1.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0",
    "less": "^4.1.2",
    "less-loader": "^6.0.0",
    "vue": "^3.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
